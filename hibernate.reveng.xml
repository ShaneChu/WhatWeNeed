<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<table name="article" catalog="community" class="article">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="identify" catalog="community" class="identify">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="revert" catalog="community" class="revert">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="subcommunity" catalog="community"
		class="subcommunity">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
<table name="user" catalog="community" class="user">
	<primary-key>
		<generator class="increment"></generator>
	</primary-key>
</table>
</hibernate-reverse-engineering>